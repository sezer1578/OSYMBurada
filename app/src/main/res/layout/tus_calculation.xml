<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:ads="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <include
        android:id="@+id/toolbar"
        layout="@layout/toolbar">

    </include>


<GridLayout
    android:layout_marginRight="5dp"
    android:layout_marginLeft="5dp"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:columnCount="3"
    android:rowCount="4"
    android:visibility="visible">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="(120)Temel Tıp Bilimleri:"
        android:textStyle="bold"
        android:textSize="15sp"
        android:textColor="@android:color/black"
        android:layout_marginRight="20dp"

        />

    <EditText

        android:id="@+id/temelTıpDogruId"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:hint="Doğru"
        android:textStyle="bold"
        android:textSize="15sp"
        android:textColor="@android:color/black"
        android:inputType="number"
        android:imeOptions="actionNext"
        />
    <EditText

        android:id="@+id/temelTıpYanlisId"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:hint="Yanlış"
        android:textStyle="bold"
        android:textSize="15sp"
        android:textColor="@android:color/black"
        android:inputType="number"
        android:imeOptions="actionNext"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="(120)Klinik Tıp Bilimleri:"
        android:textStyle="bold"
        android:textColor="@android:color/black"
        android:textSize="15sp"
        android:layout_marginRight="20dp"

        />


    <EditText

        android:id="@+id/klinikTıpDogruId"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:hint="Doğru"
        android:textStyle="bold"
        android:textSize="15sp"
        android:textColor="@android:color/black"
        android:inputType="number"
        android:imeOptions="actionNext"
        />
    <EditText

        android:id="@+id/klinikTıpYanlisId"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:hint="Yanlış"
        android:textStyle="bold"
        android:textSize="15sp"
        android:textColor="@android:color/black"
        android:inputType="number"
        android:imeOptions="actionGo"
        />



</GridLayout>

    <GridLayout
        android:layout_gravity="center"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:columnCount="2"
        android:rowCount="2"
        android:visibility="visible"
        >

        <Button
            android:id="@+id/tıpHesapla"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="HESAPLA"
            android:textSize="15sp"
            android:layout_marginBottom="5dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/button_background"
            android:layout_marginLeft="60dp"

            />

        <Button
            android:id="@+id/tipTemizle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TEMİZLE"
            android:textSize="15sp"
            android:layout_marginBottom="5dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/button_background2"
            android:layout_marginLeft="40dp"

            />
    </GridLayout>

    <TextView
        android:id="@+id/tusHesapmaSonuclariTxt"
        android:layout_marginTop="20dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="HESAPLAMA SONUÇLARI"
        android:textColor="@android:color/holo_orange_dark"
        android:textSize="20sp"
        android:layout_gravity="center"
        android:textStyle="bold"
        android:visibility="invisible"

        />

    <GridLayout
        android:layout_marginRight="5dp"
        android:layout_marginLeft="5dp"
        android:id="@+id/netGrid"
        android:layout_marginTop="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:columnCount="2"
        android:rowCount="5"
        android:visibility="invisible"
        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Temel Tıp Bilimleri Neti:"
            android:textStyle="bold"
            android:textSize="15sp"
            android:textColor="@android:color/black"
            android:layout_marginRight="20dp"
            />
        <TextView
            android:id="@+id/temelTıpNetid"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textStyle="bold"
            android:textSize="15sp"
            android:textColor="@android:color/black"
            android:layout_marginRight="20dp"
            android:maxLength="4"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Klinik Tıp Bilimleri Neti:"
            android:textStyle="bold"
            android:textSize="15sp"
            android:textColor="@android:color/black"
            android:layout_marginRight="20dp"
            />
        <TextView
            android:id="@+id/klinikTıpNetid"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textStyle="bold"
            android:textSize="15sp"
            android:textColor="@android:color/black"
            android:layout_marginRight="20dp"
            android:maxLength="4"
            />
    </GridLayout>

       <TextView
           android:layout_marginLeft="5dp"
           android:layout_marginRight="5dp"
           android:layout_gravity="center"
           android:id="@+id/mezun1Txt"
           android:layout_marginTop="15dp"
           android:layout_marginBottom="15dp"
           android:layout_width="match_parent"
           android:layout_height="wrap_content"
           android:text="TIP FAKÜLTESİ MEZUNU ADAYLAR İÇİN PUANLAR"
           android:textSize="15sp"
           android:textStyle="bold"
           android:textColor="@android:color/holo_orange_dark"
           android:visibility="invisible"
           />

    <GridLayout
        android:layout_marginRight="5dp"
        android:layout_marginLeft="5dp"
        android:id="@+id/puan1Grid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:columnCount="2"
        android:rowCount="4"
        android:visibility="invisible"
        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Ağırlıklı Klinik Tıp Bilimleri (K) Puanı:"
            android:textStyle="bold"
            android:textSize="15sp"
            android:textColor="@android:color/black"
            android:layout_marginRight="20dp"
            />
        <TextView
            android:id="@+id/kPuaniid"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textStyle="bold"
            android:textSize="15sp"
            android:textColor="@android:color/black"
            android:layout_marginRight="20dp"
            android:maxLength="4"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Ağırlıklı Temel Tıp Bilimleri (T) Puanı:"
            android:textStyle="bold"
            android:textSize="15sp"
            android:textColor="@android:color/black"
            android:layout_marginRight="20dp"
            />
        <TextView
            android:id="@+id/tPuaniid"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textStyle="bold"
            android:textSize="15sp"
            android:textColor="@android:color/black"
            android:layout_marginRight="20dp"
            android:maxLength="4"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Aile Hekimliği Uzmanlık Eğitimi (A):"
            android:textStyle="bold"
            android:textSize="15sp"
            android:textColor="@android:color/black"
            android:layout_marginRight="20dp"
            />
        <TextView
            android:id="@+id/ailePuanid"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textStyle="bold"
            android:textSize="15sp"
            android:textColor="@android:color/black"
            android:layout_marginRight="20dp"
            android:maxLength="4"
            />


    </GridLayout>

    <TextView
        android:layout_gravity="center"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:id="@+id/mezun2Txt"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:layout_marginBottom="15dp"
        android:text="TIP DIŞI MESLEK MESUBU ADAYLAR İÇİN PUANLAR"
        android:textSize="15sp"
        android:textStyle="bold"
        android:textColor="@android:color/holo_orange_dark"
        android:visibility="invisible"
        />

    <GridLayout
        android:layout_marginRight="5dp"
        android:layout_marginLeft="5dp"
        android:id="@+id/puan2Grid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:columnCount="2"
        android:rowCount="2"
        android:visibility="invisible"
        android:layout_marginBottom="10dp"
        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Ağırlıklı Temel Tıp Bilimleri (T) Puanı:"
            android:textStyle="bold"
            android:textSize="15sp"
            android:textColor="@android:color/black"
            android:layout_marginRight="20dp"
            />
        <TextView
            android:id="@+id/k2Puanid"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textStyle="bold"
            android:textSize="15sp"
            android:textColor="@android:color/black"
            android:layout_marginRight="20dp"
            android:maxLength="4"
            />


    </GridLayout>

    <com.google.android.gms.ads.AdView
        android:id="@+id/adView_tus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        ads:adSize="SMART_BANNER"
        ads:adUnitId="ca-app-pub-7273254954010969/1636040094">

    </com.google.android.gms.ads.AdView>








</LinearLayout>